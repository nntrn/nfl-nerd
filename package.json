{
  "name": "nfl-nerd",
  "version": "1.0.1",
  "description": "espn data fetcher / web scraper for nfl player prop bets",
  "repository": "github:nntrn/nfl-nerd",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./lib/*": "./lib/*.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "index": "node scripts/create-index",
    "predocs": "rm -rf ./docs",
    "jsdocs": "jsdoc -c conf.js",
    "docs": "documentation build lib/** -f html -o docs",
    "docs:api": "jsdoc . -t templates/haruki -d console -q format=json -c ./conf.js"
  },
  "keywords": [
    "nfl",
    "espn",
    "player props",
    "first TD",
    "web scrape",
    "fetch",
    "betting"
  ],
  "author": "annie tran (https://github.com/nntrn)",
  "license": "MIT",
  "dependencies": {
    "find-cache-dir": "^3.3.1",
    "node-fetch": "^2.6.1",
    "papaparse": "^5.3.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "jest": "^26.5.3"
  }
}
